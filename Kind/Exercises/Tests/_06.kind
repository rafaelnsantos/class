Tests._06(
  inc: Inte -> Inte, 
  dec: Inte -> Inte, 
  dbl: Inte -> Inte, 
  add: Inte -> Inte -> Inte, 
  mul: Inte -> Inte -> Inte, 
  sub: Inte -> Inte -> Inte, 
  eql: Inte -> Inte -> Bool, 
  gtn: Inte -> Inte -> Bool, 
  ltn: Inte -> Inte -> Bool, 
  min: Inte -> Inte -> Inte, 
  max: Inte -> Inte -> Inte, 
  hlf: Inte -> Inte, 
  div: Inte -> Inte -> Inte)
: IO<Unit>
  IO {
    let p0 = Inte.from_nat(true, 0)
    let p1 = Inte.from_nat(true, 1)
    let p5 = Inte.from_nat(true, 5)
    let p10 = Inte.from_nat(true, 10)
    let n1 = Inte.from_nat(false, 1)
    let n5 = Inte.from_nat(false, 5)
    let n10 = Inte.from_nat(false, 10)

    Assert.inte("inc +0", inc(p0), Inte.from_nat(true, 1))
    Assert.inte("inc +1", inc(p1), Inte.from_nat(true, 2))
    Assert.inte("inc -1", inc(n1), Inte.from_nat(true, 0))
    Assert.inte("inc -5", inc(n5), Inte.from_nat(false, 4))

    Assert.inte("dec +0", dec(p0), Inte.from_nat(false, 1))
    Assert.inte("dec +1", dec(p1), Inte.from_nat(true, 0))
    Assert.inte("dec -1", dec(n1), Inte.from_nat(false, 2))
    Assert.inte("dec -5", dec(n5), Inte.from_nat(false, 6))

    Assert.inte("dbl +0", dbl(p0), Inte.from_nat(true, 0))
    Assert.inte("dbl +1", dbl(p1), Inte.from_nat(true, 2))
    Assert.inte("dbl +5", dbl(p5), Inte.from_nat(true, 10))
    Assert.inte("dbl -1", dbl(n1), Inte.from_nat(false, 2))
    Assert.inte("dbl -5", dbl(n5), Inte.from_nat(false, 10))

    Assert.inte("add +0 +0", add(p0, p0), Inte.from_nat(true, 0))
    Assert.inte("add +0 +1", add(p0, p1), Inte.from_nat(true, 1))
    Assert.inte("add +5 +1", add(p5, p1), Inte.from_nat(true, 6))
    Assert.inte("add +10 -1", add(p10, n1), Inte.from_nat(true, 9))
    Assert.inte("add +5 -10", add(p5, n10), Inte.from_nat(false, 5))
    Assert.inte("add +0 -1", add(p0, n1), Inte.from_nat(false, 1))
    Assert.inte("add -5 -5", add(n5, n5), Inte.from_nat(false, 10))
    Assert.inte("add -5 +10", add(n5, p10), Inte.from_nat(true, 5))
    Assert.inte("add -5 +1", add(n5, p1), Inte.from_nat(false, 4))

    Assert.inte("mul -5 +1", mul(n5, p1), Inte.from_nat(false, 5))
    Assert.inte("mul -5 -1", mul(n5, n1), Inte.from_nat(true, 5))
    Assert.inte("mul +5 +1", mul(p5, p1), Inte.from_nat(true, 5))
    Assert.inte("mul +5 -1", mul(p5, n1), Inte.from_nat(false, 5))
    Assert.inte("mul +5 -10", mul(p5, n10), Inte.from_nat(false, 50))
    Assert.inte("mul -5 +5", mul(n5, p5), Inte.from_nat(false, 25))

    Assert.inte("sub +0 +0", sub(p0, p0), Inte.from_nat(true, 0))
    Assert.inte("sub +0 +1", sub(p0, p1), Inte.from_nat(false, 1))
    Assert.inte("sub +5 +1", sub(p5, p1), Inte.from_nat(true, 4))
    Assert.inte("sub +10 -1", sub(p10, n1), Inte.from_nat(true, 11))
    Assert.inte("sub +5 -10", sub(p5, n10), Inte.from_nat(true, 15))
    Assert.inte("sub +0 -1", sub(p0, n1), Inte.from_nat(true, 1))
    Assert.inte("sub -5 -5", sub(n5, n5), Inte.from_nat(true, 0))
    Assert.inte("sub -5 +10", sub(n5, p10), Inte.from_nat(false, 15))
    Assert.inte("sub -5 +1", sub(n5, p1), Inte.from_nat(false, 6))

    Assert.bool("eql -5 +1", eql(n5, p1), false)
    Assert.bool("eql -5 -5", eql(n5, n5), true)
    Assert.bool("eql -5 +5", eql(n5, p5), false)
    Assert.bool("eql +5 +5", eql(p5, p5), true)
    Assert.bool("eql +0 -1", eql(p0, n1), false)
    Assert.bool("eql +5 +10", eql(p5, p10), false)
    Assert.bool("eql -5 -10", eql(n5, n10), false)

    Assert.bool("gtn -5 +1", gtn(n5, p1), false)
    Assert.bool("gtn -1 -5", gtn(n1, n5), true)
    Assert.bool("gtn -1 +0", gtn(n1, p0), false)
    Assert.bool("gtn +0 -1", gtn(p0, n1), true)
    Assert.bool("gtn +1 -5", gtn(p1, n5), true)

    Assert.bool("ltn -5 +1", ltn(n5, p1), true)
    Assert.bool("ltn -1 -5", ltn(n1, n5), false)
    Assert.bool("ltn -1 +0", ltn(n1, p0), true)
    Assert.bool("ltn +0 -1", ltn(p0, n1), false)
    Assert.bool("ltn +1 -5", ltn(p1, n5), false)

    Assert.inte("min -5 +1", min(n5, p1), n5)
    Assert.inte("min +5 +1", min(p5, p1), p1)
    Assert.inte("min +5 -10", min(p5, n10), n10)
    Assert.inte("min -5 -10", min(n5, n10), n10)

    Assert.inte("max -5 +1", max(n5, p1), p1)
    Assert.inte("max +5 +1", max(p5, p1), p5)
    Assert.inte("max +5 -10", max(p5, n10), p5)
    Assert.inte("max -5 -10", max(n5, n10), n5)

    Assert.inte("hlf -10", hlf(n10), n5)
    Assert.inte("hlf -5", hlf(n5), Inte.from_nat(false, 2))
    Assert.inte("hlf -1", hlf(n1), p0)
    Assert.inte("hlf +10", hlf(p10), p5)
    Assert.inte("hlf +5", hlf(p5), Inte.from_nat(true, 2))
    Assert.inte("hlf +1", hlf(p1), p0)

    Assert.inte("div -5 +1", div(n5, p1), Inte.from_nat(false, 5))
    Assert.inte("div -5 -1", div(n5, n1), Inte.from_nat(true, 5))
    Assert.inte("div +5 +1", div(p5, p1), Inte.from_nat(true, 5))
    Assert.inte("div +5 -1", div(p5, n1), Inte.from_nat(false, 5))
    Assert.inte("div +5 -10", div(p5, n10), Inte.from_nat(true, 0))
    Assert.inte("div -5 +5", div(n5, p5), Inte.from_nat(false, 1))
    Assert.inte("div -10 +5", div(n10, p5), Inte.from_nat(false, 2))
    Assert.inte("div +10 +5", div(p10, p5), Inte.from_nat(true, 2))

    IO.print("Finished")
  }