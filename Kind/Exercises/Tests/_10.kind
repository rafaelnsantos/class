Tests._10.is_upper(is_upper: Char -> Bool): IO<Unit>
  IO {
    Assert.bool("is_upper a", is_upper('a'), false)
    Assert.bool("is_upper A", is_upper('A'), true)
    Assert.bool("is_upper 0", is_upper('0'), false)
    IO.print("is_upper: All terms tested")
  }

